<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextArea prefHeight="68.0" prefWidth="506.0" />
   <PasswordField />
   <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image fx:id="i_image" url="@Image/anh2.jpg" />
      </image>
   </ImageView>
   <CheckBox mnemonicParsing="false" text="CheckBox" />

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" style="-fx-background-color: #123456; -fx-text-fill: #fff; -fx-border-color: none; -fx-background-radius: 10px;" text="Hello!" />
</VBox>
